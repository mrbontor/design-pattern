{
    "title": "An object value",
    "required": [
        "name",
        "address",
        "phone"
    ],
    "properties": {
        "name": {
            "$id": "#/properties/name",
            "title": "The name Schema",
            "pattern": "^(.*)$",
            "type": "string"
        },
        "address": {
            "$id": "#/properties/address",
            "title": "The name Schema",
            "pattern": "^(.*)$",
            "type": "string"
        },
        "phone": {
            "$id": "#/properties/phone",
            "title": "The phone Schema",
            "default": "",
            "minLength": 10,
            "maxLength": 14,
            "type": "string",
            "examples": [
                "6285206458902"
            ],
            "pattern": "^(\\0 ((\\d{3}([ -]\\d{3,})([- ]\\d{4,})?)|(\\d+)))|(\\(\\d+\\) \\d+)|\\d{3}( \\d+)+|(\\d+[ -]\\d+)|\\d+"
        },
        "bio": {
            "$id": "#/properties/bio",
            "title": "The name Schema",
            "pattern": "^(.*)$",
            "type": "string"
        }
    },
    "additionalProperties": false,
    "$id": "http://example.org/post.json#",
    "type": "object",
    "$schema": "http://json-schema.org/draft-07/schema#"
}
